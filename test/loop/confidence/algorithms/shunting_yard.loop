# Recursive shunting yard algorithm

shunt(input, symbols, ops) =>
  /[0-9]+/, *, *             : symbols.add(input)
  /[+-/*%]+/, *, *           : ops.add(input)
  *, *, *                    : 'error'

parse(input) ->
  symbols: [],
  ops: [],
  do_shunt: shunt(i, symbols, ops) for i in input,
  [symbols, ops]

main ->
  parse(['1', '+', '2', '-', '3'])
